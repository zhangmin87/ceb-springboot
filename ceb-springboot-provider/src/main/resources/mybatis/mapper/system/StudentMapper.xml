<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ceb.system.mapper.StudentMapper">
    <select id="get" resultType="com.ceb.system.DTO.StudentDTO">
        SELECT
        t.name AS name,
        t.sno  AS sno
        FROM student t
--         WHERE t.sno = '32'
    </select>
</mapper>